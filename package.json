{
  "name": "cetec-design-system",
  "version": "0.0.41-0",
  "description": "Cetec-ERP Design System",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "prepare": "npx panda codegen",
    "dev": "npx panda codegen --watch & vite",
    "build": "rm -rf dist && PANDA_STATIC=true npx panda codegen && npx panda cssgen --splitting && npx panda spec && npx panda init-mcp --client claude && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "generate-sprite": "node src/utils/generate-sprite.js",
    "storybook": "PANDA_STATIC=true storybook dev -p 6006",
    "storybook:build": "PANDA_STATIC=true storybook build",
    "panda-mcp": "npx panda mcp"
  },
  "files": [
    "./dist/",
    "./src/styles/*",
    "./src/recipes/*",
    "./src/tokens/*",
    "./src/utils/*.ts"
  ],
  "peerDependencies": {
    "@fontsource-variable/recursive": "^5.1.1",
    "@fontsource-variable/ruda": "^5.1.1",
    "react": "19.0.0",
    "react-dom": "19.0.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@pandacss/dev": "^1.8.0",
    "@storybook/addon-essentials": "^8.6.15",
    "@storybook/addon-interactions": "^8.6.15",
    "@storybook/addon-themes": "^8.6.15",
    "@storybook/blocks": "^8.6.15",
    "@storybook/react": "^8.6.15",
    "@storybook/react-vite": "^8.6.15",
    "@storybook/test": "^8.6.15",
    "@types/node": "^22.15.30",
    "@types/react": "19.0.8",
    "@types/react-dom": "19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "auto": "^11.3.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "eslint-plugin-storybook": "^0.11.2",
    "globals": "^15.14.0",
    "postcss": "^8.5.1",
    "prettier": "^3.3.3",
    "storybook": "^8.6.15",
    "svg-sprite": "^2.0.4",
    "svgo": "^3.0.2",
    "typescript": "~5.7.3",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.5",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-static-copy": "^3.1.4"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  },
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./preset": {
      "types": "./dist/types/preset.d.ts",
      "import": "./dist/preset.js",
      "default": "./dist/preset.js"
    },
    "./sprite.svg": "./dist/sprite.svg",
    "./styles.css": "./dist/styles.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Cetec-ERP/Cetec-Design-System.git"
  },
  "author": "Cetec-ERP",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Cetec-ERP/Cetec-Design-System/issues"
  },
  "homepage": "https://github.com/Cetec-ERP/Cetec-Design-System#readme"
}
